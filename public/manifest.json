{
  "manifest_version": 2,
  "name": "Beautiful Bookmarks",
  "description": "An extension for managing bookmarks.",
  "version": "1.0.2",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Bookmarks"
  },
  "permissions": [
    "storage", "tabs", "bookmarks", "chrome://favicon/"
  ],
  "icons": {
    "64": "bookmark.png"
  }
}