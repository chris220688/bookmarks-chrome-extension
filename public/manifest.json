{
  "manifest_version": 2,
  "name": "Kolitiri Bookmarks",
  "version": "1.0.2",
  "description": "An extension for managing bookmarks.",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Kolitiri Bookmarks"
  },
  "permissions": [
    "storage", "tabs", "bookmarks", "chrome://favicon/"
  ],
  "icons": {
    "64": "bookmark.png",
    "128": "bookmark-128.png"
  }
}