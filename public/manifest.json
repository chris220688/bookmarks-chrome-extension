{
  "manifest_version": 2,
  "name": "Bookmarks",
  "description": "An extension for managing bookmarks.",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Bookmarks"
  },
  "permissions": [
    "storage", "tabs", "bookmarks"
  ],
  "icons": {
    "64": "bookmark.png"
  }
}